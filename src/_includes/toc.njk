<div class="grid contents">
    <p class="start7 end13">Table of Contents</p>
    {% for chapter in collections.chapter %}
        <div class="grid start1 end13 chapter">
            <p class="start1 end7">{{ loop.index }}</p>
            <h1 class="start7 end13">{{ chapter.data.title }}</h1>
        </div>
    {% endfor %}
</div>